package com.lf;

public class Person implements Eatable{
	
	public int age = 2;
	private int count = 10;
	
	
	public void eat(Eatable e) {
		System.out.println(
				"eat - " + e.name()
				+ " - " + e.energy());
	}
	
	public void happy() {
		eat(new Eatable() {
			
			{
				System.out.println("Happy New Year");
			}
			
			@Override
			public String name() {
				count = 6;
				return null;
			}
			
			@Override
			public int energy() {
				return 0;
			}
		});
	}

	
	@Override
	public String name() {
		return "Apple";
	}

	@Override
	public int energy() {
		return 150;
	}
	
	
	
}
