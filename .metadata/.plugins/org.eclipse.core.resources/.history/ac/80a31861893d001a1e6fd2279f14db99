package com.lf.regex;

public class Main {

	public static void main(String[] args) {

//		System.out.println("haha");
		
		String regex = ".";
		System.out.println("c".matches(regex));
		System.out.println("1".matches(regex));
		System.out.println(".".matches(regex));
		
		String regex = ".";
		
		

	}

}
