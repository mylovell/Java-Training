package com.lf.regex;

public class Quantifier {

	public static void main(String[] args) {

		String regex = "6{2}";
		System.out.println("6".matches(regex));
		System.out.println("66".matches(regex));
		System.out.println("666".matches(regex));
		System.out.println("-------------------");

		String regex2 = "6{2}?";
		System.out.println("6".matches(regex2));
		System.out.println("66".matches(regex2));
		System.out.println("666".matches(regex2));
		System.out.println("-------------------");
		
		
	}

}
