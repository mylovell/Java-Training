package com.lf;



public class Person implements Eatable{
	
	public int age = 2;
	
	public Person() {
		// TODO Auto-generated constructor stub
//		age = 5;
	}
	
	public void eat(Eatable e) {
		System.out.println(
				"eat - " + e.name()
				+ " - " + e.energy());
	}

	@Override
	public String name() {
		// TODO Auto-generated method stub
		return "Apple";
	}

	@Override
	public int energy() {
		// TODO Auto-generated method stub
		return 150;
	}
	
	
	
}
