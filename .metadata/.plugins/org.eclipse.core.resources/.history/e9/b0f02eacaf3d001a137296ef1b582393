package com.lf.regex;

public class Group {

	public static void main(String[] args) {
		
//		group();
		backreference();
	}
	
	public static void group() {
		String regex = "(dog){2}";
		System.out.println("dogdog".matches(regex));
	}
	
	public static void backreference() {
		String regex = "(\\d\\d)\\3";
		System.out.println("1212".matches(regex));
	}

}
