package com.supplier;

public class Tool {
	
	String s1 = "A";
	String s2 = "B";
	String s3 = "C";
	
	
	String getFirstNotEmptyString(String s1, Supplier<String> s2) {
		if (s1 != null && s1.length() != 0) return s1;
		if (s2 == null) return null;
		String str = s2.get();
		return (str != null && str.length() != 0) ? str : null;
	}

}
