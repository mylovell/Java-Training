package com.lf.regex;

import java.util.regex.Pattern;

public class DOTALL {

	public static void main(String[] args) {
		
		CASE();
	}
	
	public static void CASE() {
		
		String regex = ".";
		String input = "\r\n";
		Matcher.findAll(regex, input);
		System.out.println("----------------");
		
		Matcher.findAll(regex, input, Pattern.DOTALL);
		System.out.println("----------------");
		
	}

}
