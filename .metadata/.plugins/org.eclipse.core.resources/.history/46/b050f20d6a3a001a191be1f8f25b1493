package com.lf;

public class Person {
	private static int count = 1;
	private static void run() {
		System.out.println("Person - run");
	}
	
	public static class Car {
		public void log() {
			// 静态嵌套类可以直接访问外部类中的成员
			Person.count = 2;
			Person.run();
			run();
			System.out.println("Car - Log " + count);
		}
	}
}
